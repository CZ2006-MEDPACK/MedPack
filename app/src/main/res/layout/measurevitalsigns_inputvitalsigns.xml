<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">

    <RadioButton
        android:id="@+id/radioButton4"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_alignTop="@+id/textView6"
        android:layout_toEndOf="@+id/textView11"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:text="3"
        android:textColor="@android:color/black" />

    <RadioButton
        android:id="@+id/radioButton5"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_alignTop="@+id/textView6"
        android:layout_toEndOf="@+id/radioButton4"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:text="4"
        android:textColor="@android:color/black" />

    <RadioButton
        android:id="@+id/radioButton6"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_alignTop="@+id/textView6"
        android:layout_toEndOf="@+id/radioButton5"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:text="5"
        android:textColor="@android:color/black" />

    <RadioButton
        android:id="@+id/radioButton3"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_alignEnd="@+id/textView11"
        android:layout_alignTop="@+id/textView6"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:text="2"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="236dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="46dp"
        android:text="Your Vital Signs"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@android:color/black"/>

    <EditText
        android:id="@+id/editText10"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="244dp"
        android:ems="10" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="260dp"
        android:ems="10" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView7"
        android:ems="10" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_toEndOf="@+id/textView3"
        android:ems="10"
        android:text="100"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="244dp"
        android:ems="10"
        android:text="Name" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="357dp"
        android:layout_height="98dp"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-200dp"
        android:ems="10"
        android:inputType="textLongMessage" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginBottom="200dp"
        android:text="Any Symptoms?"
        android:textColor="@android:color/black"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginBottom="234dp"
        android:text="Enter Pain Scale:"
        android:textColor="@android:color/black"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView3"
        android:layout_centerVertical="true"
        android:text="Enter Blood Pressure: "
        android:textColor="@android:color/black"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView3"
        android:layout_alignTop="@+id/editText4"
        android:text="Enter Respiratory Rate: "
        android:textColor="@android:color/black"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="203dp"
        android:text="Enter Temperature: "
        android:textColor="@android:color/black"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="125dp"
        android:text="Your Oxygen Saturation: "
        android:textColor="@android:color/black"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="164dp"
        android:text="Please Enter Below ..."
        android:textColor="@android:color/black"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="98dp"
        android:text="Your Pulse Rate:"
        android:textColor="@android:color/black"/>

    <EditText
        android:id="@+id/editText"
        android:layout_width="209dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_toEndOf="@+id/textView3"
        android:ems="10"
        android:singleLine="true"
        android:text="68"
        android:textSize="12sp" />

    <RadioButton
        android:id="@+id/radioButton2"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_alignStart="@+id/editText4"
        android:layout_alignTop="@+id/textView6"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:text="1"
        android:textColor="@android:color/black"/>

    <Button
        android:id="@+id/button"
        android:layout_width="178dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="21dp"
        android:text="Submit" />

</RelativeLayout>